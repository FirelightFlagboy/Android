<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="icon_menu">
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:background">@mipmap/ic_menu_white_48dp</item>
    </style>
    <style name="icon_option">
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:background">@mipmap/ic_more_vert_white_48dp</item>
    </style>
    <style name="ic_retour">
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:background">@mipmap/ic_arrow_back_white_48dp</item>
    </style>
    <style name="title_msg">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>


</resources>
