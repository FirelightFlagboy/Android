<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lord_arduino.fr.elite_golf"
    android:versionCode="1"
    android:versionName="2.8" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.lord_arduino.fr.elite_golf.Menu_principal" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.lord_arduino.fr.elite_golf.param_herbe"
            android:label="@string/parm_herb" >
        </activity>
        <activity
            android:name="com.lord_arduino.fr.elite_golf.parcour"
            android:label="@string/c_parc" >
        </activity>
        <activity
            android:name="com.lord_arduino.fr.elite_golf.statistique"
            android:label="@string/statis" >
        </activity>
        <activity
            android:name="com.lord_arduino.fr.elite_golf.information"
            android:label="@string/info" >
        </activity>
        <activity
            android:name="com.lord_arduino.fr.elite_golf.bluetooth"
            android:label="@string/bleut" >
        </activity>
        <activity
            android:name="com.lord_arduino.fr.elite_golf.parametre"
            android:label="@string/param" >
        </activity>
        <activity
            android:name="com.lord_arduino.fr.elite_golf.couleur"
            android:label="Couleur" >
        </activity>

        <provider
            android:name="com.android.tools.fd.runtime.InstantRunContentProvider"
            android:authorities="com.lord_arduino.fr.elite_golf.com.android.tools.fd.runtime.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>